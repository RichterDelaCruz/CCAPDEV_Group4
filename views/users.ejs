<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>

<body>
    <nav>
        <ul>
            <li><a href="/homepage">Homepage</a></li>
            <li><a href="/users">Search Users</a></li>
        </ul>
    </nav>

    <h1>List of Users</h1>
    <ul>
        <% users.forEach((user) => { %>
        <li>
            <a href="/profile/<%= user._id %>">
                <img src="<%= user.profilePicture %>" alt="<%= user.username %>">
                <%= user.username %>
            </a>
 </li>
        <% }); %>
    </ul>
      
</body>

</html>